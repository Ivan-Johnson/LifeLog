server {
    server_name lifelog.ivanjohnson.net;
    root /srv/LifeLog;

    index index.html;
    location /api/v1 {
        include         uwsgi_params;
        uwsgi_pass      unix:/tmp/uwsgi.socket;
    }

    listen [::]:443 ssl http2;
    listen 443 ssl http2;
    listen 80;
    listen [::]:80;

    gzip off;

    ##
    # Logging Settings
    ##
    access_log /srv/LifeLog/Logaccess.log;
    error_log  /srv/LifeLog/Logerror.log;
}

